{
  "name": "@aw-web-design/components",
  "version": "0.0.13",
  "description": "A component library",
  "scripts": {
    "build:tokens": "node ./node_modules/@aw-web-design/theme/generate",
    "lint": "yarn lint:js && yarn lint:styled",
    "lint:js": "eslint -c .eslintrc packages --ext .ts,.tsx --quiet",
    "lint:styled": "stylelint \"packages/**/styled.ts\"",
    "test": "jest --silent -u --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AW-Web-Design/Components.git"
  },
  "author": "Andy Wilson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AW-Web-Design/Components/issues"
  },
  "homepage": "https://github.com/AW-Web-Design/Components#readme",
  "dependencies": {
    "@aw-web-design/theme": "0.0.27",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^4.17.0",
    "copy-webpack-plugin": "^8.0.0",
    "csstype": "3.0.7",
    "enhanced-resolve": "5.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.2.0",
    "styled-system": "5.1.5",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-docs": "6.1.21",
    "@types/webpack": "^4.41.22",
    "@types/webpack-env": "^1.15.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "circular-dependency-plugin": "5.2.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "5.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "html-webpack-plugin": "^5.3.0",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "mini-css-extract-plugin": "1.3.9",
    "prettier": "2.1.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "8.0.17",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^5.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/^/node_modules/@orchard/((theme)|(theme-provider))$/"
    ],
    "resolver": "./jestAliasResolver"
  }
}
